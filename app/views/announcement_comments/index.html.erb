<p id="notice"><%= notice %></p>

<h1>Listing Announcement Comments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @announcement_comments.each do |announcement_comment| %>
      <tr>
        <td><%= link_to 'Show', announcement_comment %></td>
        <td><%= link_to 'Edit', edit_announcement_comment_path(announcement_comment) %></td>
        <td><%= link_to 'Destroy', announcement_comment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Announcement comment', new_announcement_comment_path %>
